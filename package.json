{
  "name": "gokaygurcan.github.io",
  "version": "2.0.0-alpha",
  "description": "Personal website",
  "homepage": "https://www.gokaygurcan.com/",
  "repository": "gokaygurcan/gokaygurcan.github.io",
  "author": {
    "name": "Gökay Gürcan",
    "email": "info@gokaygurcan.com",
    "url": "https://www.gokaygurcan.com/"
  },
  "scripts": {
    "build": "next build",
    "dev": "next",
    "heroku-postbuild": "npm run build",
    "precommit": "npm test",
    "start": "next start -p $PORT",
    "test": "xo && jest -u --coverage"
  },
  "copyright": "2017",
  "license": "MIT",
  "bugs": "https://github.com/gokaygurcan/gokaygurcan.github.io/issues",
  "keywords": [
    "personal",
    "website"
  ],
  "dependencies": {
    "next": "^2.2.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint-config-xo-react": "^0.13.0",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.3",
    "jest-cli": "^20.0.4",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "xo": "^0.18.1"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "test": {
        "presets": [
          "es2015",
          "next/babel"
        ]
      }
    }
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "envs": [],
    "global": [
      "describe",
      "expect",
      "global",
      "it",
      "test"
    ],
    "rules": {
      "brace-style": [
        "error",
        "stroustrup",
        {
          "allowSingleLine": true
        }
      ],
      "camelcase": 0,
      "capitalized-comments": 0,
      "new-cap": 0,
      "react/jsx-space-before-closing": 0,
      "react/jsx-tag-spacing": 0
    },
    "ignore": [],
    "overrides": [],
    "extends": [
      "xo-react"
    ],
    "plugins": []
  }
}
