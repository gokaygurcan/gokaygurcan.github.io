{
  "name": "gokaygurcan.github.io",
  "version": "0.1.0",
  "description": "Personal website",
  "homepage": "https://www.gokaygurcan.com/",
  "scripts": {
    "start": "babel-node src/server",
    "dev:start": "nodemon --exec babel-node src",
    "test": "xo && flow && jest --coverage",
    "precommit": "yarn test"
  },
  "repository": "gokaygurcan/gokaygurcan.github.io",
  "author": {
    "name": "Gökay Gürcan",
    "email": "info@gokaygurcan.com",
    "url": "https://www.gokaygurcan.com/"
  },
  "copyright": "2017",
  "license": "MIT",
  "bugs": "https://github.com/gokaygurcan/gokaygurcan.github.io/issues",
  "keywords": [
    "personal",
    "website"
  ],
  "dependencies": {
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "express": "^5.0.0-alpha.5",
    "express-session": "^1.15.1",
    "morgan": "^1.8.1",
    "node-sass-middleware": "^0.11.0",
    "serve-favicon": "^2.4.1",
    "toobusy-js": "^0.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "eslint": "^3.17.1",
    "eslint-config-xo": "^0.17.0",
    "eslint-plugin-flowtype": "^2.30.2",
    "eslint-plugin-flowtype-errors": "^3.0.1",
    "flow-bin": "^0.41.0",
    "husky": "^0.13.2",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "global": [
      "expect",
      "test"
    ],
    "rules": {
      "flowtype-errors/show-errors": 2
    },
    "extends": [
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype",
      "flowtype-errors"
    ]
  }
}
