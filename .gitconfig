# gokaygurcan/gokaygurcan.github.io

[branch "develop"]
  remote = origin
  merge = refs/heads/develop

[branch "master"]
  remote = origin
  merge = refs/heads/master

[color]
  ui = auto

[core]
  autocrlf = input

[fetch]
  prune = true

[gitflow "branch"]
  master = master
  develop = develop

[gitflow "prefix"]
  feature = feature/
  release = release/
  hotfix = hotfix/
  support = support/
  versiontag = v

[merge]
  ff = only

[push]
  default = matching

[rebase]
  autosquash = true

[remote "origin"]  
  url = git@github.com:gokaygurcan/gokaygurcan.github.io.git
  fetch = +refs/heads/*:refs/remotes/origin/*
